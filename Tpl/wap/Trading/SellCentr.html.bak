<script>
    //挂卖滚动
    var height = $(document.body).height();
    var vheight = height - $(".celchong").height() * 2 - $(".chongzhiinput").height() - $(".titlediv").height() * 1.2 - $(".yxui-flex").height();
    $("#mainlist").css('height', vheight);

    $('#DrawMoneybtn').click(function () {
        var cardid = $('#carnumber').val();//银行卡id
        if (cardid == '' || cardid == null) {
            layer.alert('请您先绑定银行卡', {
                time: 0 //不自动关闭
                , btn: ['好']
                , yes: function (index) {
                    layer.close(index);
                    window.location.href = "{:U('Growth/Cardinfos')}";
                }
            });
            return;
        }
        var num =  $('#num').val();
        var Money = $('input[name=Money]').val();
        allNum = num*(1+0.1);
        if(allNum > Money){
            layer.msg('您当前账户暂无这么多余额');
            return false;
        }
        $('.ftc_wzsf').show();
    });
    $('#num').change(function () {
        var num = $(this).val();
        poundage = num * 0.2;
        $('.DrawMoney_sxf').text(poundage);
    });

    $(function () {
        var cardid = $('#carnumber').val();//银行卡id
        if (cardid == '' || cardid == null) {
            layer.alert('请您先绑定银行卡', {
                time: 0 //不自动关闭
                , btn: ['好']
                , yes: function (index) {
                    layer.close(index);
                    window.location.href = "{:U('Growth/Cardinfos')}";
                }
            });
        }

       var num =  $('#num').val();
        poundage = num*0.2;
        $('.DrawMoney_sxf').text(poundage);

        //关闭浮动
        $(".close").click(function () {
            $(".ftc_wzsf").hide();
            $(".mm_box li").removeClass("mmdd");
            $(".mm_box li").attr("data", "");
            i = 0;
        });
        //数字显示隐藏
        $(".xiaq_tb").click(function () {
            $(".numb_box").slideUp(500);
        });
        $(".mm_box").click(function () {
            $(".numb_box").slideDown(500);
        });
        //----
        var i = 0;
        $(".nub_ggg li .zf_num").click(function () {

            if (i < 6) {
                $(".mm_box li").eq(i).addClass("mmdd");
                $(".mm_box li").eq(i).attr("data", $(this).text());
                i++
                if (i == 6) {
                    setTimeout(function () {
                        var pwd = "";
                        $(".mm_box li").each(function () {
                            pwd += $(this).attr("data");
                        });
                        //AJAX提交数据
                        var sellnums = $('#num').val();//账号  //.trim() 去空格判断
                        var cardid = $('#carnumber').val();//银行卡id
                        var messge = '';
                        var exg = /^[1-9]\d*|0$/;
                        if (!exg.test(sellnums)) {
                            msg_alert('请选择买入金额~');
                            return;
                        }
                        if (cardid == '' || cardid == null) {
                            msg_alert('请选择银行卡');
                            return;
                        }
                        $.ajax({
                            url: '/Trading/SellCentr',
                            type: 'post',
                            data: {'sellnums': sellnums, 'pwd': pwd, 'cardid': cardid, 'messge': messge},
                            datatype: 'json',
                            success: function (mes) {
                                if (mes.status == 1) {
                                    msg_alert(mes.message);
                                    $(".ftc_wzsf").hide();
                                    $(".mm_box li").removeClass("mmdd");
                                    $(".mm_box li").attr("data", "");
                                    i = 0;
                                    window.location.reload()
                                } else {
                                    msg_alert(mes.message);
                                    $(".mm_box li").removeClass("mmdd");
                                    $(".mm_box li").attr("data", "");
                                    i = 0;
                                }
                            }
                        })
                    }, 100);
                }
                ;
            }
        });

        $(".nub_ggg li .zf_del").click(function () {
            if (i > 0) {
                i--
                $(".mm_box li").eq(i).removeClass("mmdd");
                $(".mm_box li").eq(i).attr("data", "");
            }
        });

        $(".nub_ggg li .zf_empty").click(function () {
            $(".mm_box li").removeClass("mmdd");
            $(".mm_box li").attr("data", "");
            i = 0;
        });
        $('#hide').click(function () {
            $('#p1').toggle();
        });

        //公共提示框
        function msg_alert(message, url) {

            if (url) {
                layer.msg(message, {time: 1000}, function () {
                    window.location.href = url;
                });
            } else {
                layer.msg(message, {time: 1500});
            }

        }
    });
</script>
